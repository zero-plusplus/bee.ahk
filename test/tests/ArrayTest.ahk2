#Include ..\common.ahk2

class ArrayTest {
  class InstanceTest {
    testJoin(assert) {
      assert.equals(bee.Array("a", "b", "c").join(""), "abc")
      assert.equals(bee.Array("a", "b", "c").join(", "), "a, b, c")
    }
  }
  class StaticTest {
    testJoin(assert) {
      assert.equals(bee.Array.join([ "a", "b", "c" ], ""), "abc")
      assert.equals(bee.Array.join({ a: "a", b: "b", c: "c" }, ", "), "a, b, c")
    }
  }
}