#Include ..\common.ahk2

class PredicateTest {
  testAll(assert) {
    assert.ok(bee.Predicate.all("abc", [ bee.Predicate.startsWith ]))
  }
  testIsObject(assert) {
    assert.ok(bee.Predicate.isObject()({}))
    assert.ok(bee.Predicate.isObject({}))
  }
  testStartsWith(assert) {
    assert.ok(bee.Predicate.startsWith("abcdefg", "abc"))

    startsWithAbc := bee.Predicate.startsWith("abc")
    assert.ok(startsWithAbc("abcdefg"))
  }
  class AllTest {
    testAllIsObject(assert) {
      assert.ok(bee.Predicate.all.isObject([ {}, [] ]))
    }
  }
}