class PredicateTest {
  testIsObject(assert) {
    assert.ok(bee.Predicate.isObject()({}))
    assert.ok(bee.Predicate.isObject({}))
  }
  testStartsWith(assert) {
    assert.ok(bee.Predicate.startsWith("abcdefg", "abc"))

    startsWithAbc := bee.Predicate.startsWith("abc")
    assert.ok(startsWithAbc("abcdefg"))
  }
}