class PredicateFunc {
  static call(params*) {
    maxParams := this.behavior.maxParams - 1
    if (maxParams <= params.length) {
      try {
        return this.behavior(params*)
      }
      return false
    }
    if (params.length < maxParams) {
      params.insertAt(1, bee)
    }
    return bee.curry(ObjBindMethod(this, "behavior"), maxParams)(params*)
  }
}