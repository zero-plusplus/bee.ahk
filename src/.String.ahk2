class String extends bee.rule.Primitive{
  /**
   * Gets the length of the specified string.
   * @param {string} str
   * @return {number}
   */
  static length(str) => StrLen(str)
  /**
   * Gets the length of a string.
   * @param {string} str
   * @return {number}
   */
  length() => bee.String.length(this.value)
}